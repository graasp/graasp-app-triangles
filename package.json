  ],
  "license": "MIT",
  "homepage": ".",
  "dependencies": {
    "@fortawesome/fontawesome-free-solid": "5.0.13",
    "@fortawesome/fontawesome-svg-core": "1.2.8",
    "@fortawesome/free-solid-svg-icons": "5.5.0",
    "@fortawesome/react-fontawesome": "0.1.3",
    "big.js": "5.2.2",
    "bootstrap": "4.1.3",
    "cross-env": "5.2.0",
    "env-cmd": "8.0.2",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "i18next": "12.1.0",
    "i18next-browser-languagedetector": "2.2.4",
    "konva": "2.5.1",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-i18next": "8.3.8",
    "react-konva": "16.6.31",
    "react-redux": "5.1.1",
    "react-scripts": "2.1.1",
    "reactstrap": "6.5.0",
    "redux": "4.0.1"
  },
  "scripts": {
    "start": "env-cmd ./.env.local react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-cmd ./.env.dev react-scripts build",
    "build:prod": "env-cmd ./.env.prod react-scripts build",
    "build:version": "REACT_APP_VERSION=$CI_BRANCH env-cmd --no-override ./.env.prod react-scripts build",
    "lint": "eslint .",
    "test": "react-scripts test --env=jsdom",
    "test:ci": "cross-env CI=true env-cmd ./.env.test react-scripts test --env=jsdom --coverage",
    "report": "cat ./coverage/lcov.info | env-cmd ./.env.test codacy-coverage",
    "eject": "react-scripts eject",
    "release": "standard-version"
  },
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "codacy-coverage": "3.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "husky": "1.2.0",
    "standard-version": "4.4.0"
  },
  "release": {
    "analyzeCommits": "conventional"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
